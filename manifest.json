{
    "manifest_version": 3,
    "name": "JPG Downloader",
    "version": "1.0",
    "description": "Download all JPG images from the current webpage.",
    "permissions": ["downloads", "activeTab", "scripting"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    
    }
  }
  